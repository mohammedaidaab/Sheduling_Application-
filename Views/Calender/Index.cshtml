<!-- days sourced from: https://nationaldaycalendar.com/february/ -->

	@{
		Layout = null;
		int i=1;

	DateTime date = new DateTime(Model.currentYear, Model.currentMonth, 1);
	}
@model CalenderModel
<link href="~/css/calendersheet.css" rel="stylesheet" />

<h1>@date.ToString("MMMM") @Model.currentYear</h1>
<p>Schedule</p>

	<ul>
	@foreach (var obj in Model.Details)
	{
		<li><time datetime="2022-02-01">@obj.Day</time>
			@if (obj.scheduleModels != null)
			{
				@foreach(var obj2 in obj.scheduleModels)
				{
					<a asp-route-id="@obj2.Scheduleid">@obj2.Name</a><br />
				
				}
			}
		</li>
	}
